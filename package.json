{
  "name": "library-name",
  "version": "1.0.0",
  "description": "A library starter template.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils": "./dist/utils.mjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "unbuild",
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "serve": "vitepress serve docs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "commit": "cz",
    "commit:all": "git add . && cz"
  },
  "devDependencies": {
    "@apenhet/eslint-config": "latest",
    "c8": "^7.11.0",
    "commitizen": "^4.2.4",
    "cz-emoji": "^1.3.1",
    "eslint": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  },
  "eslintConfig": {
    "extends": "@apenhet"
  },
  "unbuild": {
    "declaration": true,
    "entries": [
      "./src/index"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
